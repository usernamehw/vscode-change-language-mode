{
	"name": "change-language-mode",
	"displayName": "Change Language Mode",
	"description": "Change programming language from a keybinding",
	"version": "0.0.1",
	"publisher": "usernamehw",
	"license": "MIT",
	"engines": {
		"vscode": "^1.28.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/usernamehw/vscode-change-language-mode"
	},
	"bugs": {
		"url": "https://github.com/usernamehw/vscode-change-language-mode/issues"
	},
	"keywords": [
		"change",
		"language",
		"mode"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:changeLanguageMode.change"
	],
	"main": "./out/extension",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
